{
    "name": "tianrosandhy/autocrud",
    "description": "Autocrud component to help easily generate datatable & form",
    "type": "module",
    "authors": [
        {
            "name": "Christian Rosandhy",
            "homepage": "https://tianrosandhy.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "^5|^6|^7|^8|^9"
    },
    "autoload": {
        "psr-4": {
            "TianRosandhy\\Autocrud\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "TianRosandhy\\Autocrud\\ServiceProvider"
            ],
            "_aliases": {
                "DataTableAutocrud": "TianRosandhy\\Autocrud\\Facades\\DataTableAutocrud"
            }
        }
    }
}